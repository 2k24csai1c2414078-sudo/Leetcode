#include <stdbool.h>

bool carPooling(int** trips, int tripsSize, int* tripsColSize, int capacity) {
    int bus[1001] = {0};

    for (int i = 0; i < tripsSize; i++) {
        int passenger = trips[i][0];
        int from = trips[i][1];
        int to = trips[i][2];
        bus[from] += passenger;
        bus[to] -= passenger;
    }

    int current = 0;
    for (int i = 0; i < 1001; i++) {
        current += bus[i];
        if (current > capacity) {
            return false;
        }
    }
    return true;
}
